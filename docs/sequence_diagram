shape: sequence_diagram

User -> Computer: "Futebol\n"
Computer -> Serial: "futebol em binário"
Serial -> UART_rx: uart_pool_out()
UART_rx -> thread: leitura da mensagem
thread -> UART_tx: futebol em binário
UART_tx -> Serial: callback()
Serial -> Computer: "futebol em binário"
Computer -> User: "Echo Bot: Futebol"
